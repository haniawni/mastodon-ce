- if Rails.configuration.x.frontend_default

  - content_for :header_tags do
    %script#initial-state{:type => 'application/json'}!= json_escape(render(file: 'home/initial_state', formats: :json))

    = javascript_include_tag "#{Rails.configuration.x.frontend_default}", integrity: true, crossorigin: 'anonymous'
    = stylesheet_link_tag "#{Rails.configuration.x.frontend_default}", integrity: true, media: 'all'

  = react_component 'Mastodon', default_props, class: 'app-holder', prerender: false

- else

  - content_for :header_tags do

    = javascript_include_tag "#{Rails.configuration.x.skin_default}", integrity: true, crossorigin: 'anonymous'
    = stylesheet_link_tag "#{Rails.configuration.x.skin_default}", integrity: true, media: 'all'

  %h1 Oops!

  %p This instance of Mastodon doesn't have a frontend installed!
